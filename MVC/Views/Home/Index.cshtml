@model MVC.ViewModels.AgendamentoViewModel
<main>
                   <div class="titulo">
                       <h1>Eventos</h1>
                </div>
                     <br><br>
                <div class="text">

                    <div class="imagens-home">
                        <img src="img/balada.png" title="balada monstra" > 
                        <p>Venha para o melhor evento de games. Teremos vários games novos e muito entretenimento!! </p>
                    </div>
                    <div class="imagens-home">
                        <img src="img/bgs1.png" title="bgs" >
                        <p>Venha para o melhor evento de games. Teremos vários games novos e muito entretenimento!! </p>
                    </div>
                    <div class="imagens-home">
